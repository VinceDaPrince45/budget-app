<h1><strong><%= title %></strong></h1>
<% if (expense_list.length) { %>
    <dl>
        <% expense_list.forEach(function(expense) { %>
            <dt>
                <a href="<%= expense.url %>"><%= expense.item_name %></a>
                <p>Price: $<%= expense.price %></p>
                <p>Date Bought: <%= expense.date_bought_formatted %></p>
            </dt>
        <% }); %>
    </dl>
<% } else { %>
    <p>There are no expenses.</p>
<% } %>